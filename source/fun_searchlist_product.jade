extend layout/_layout_side_r.jade

mixin search(url,img,name)
	li
		.search_data
			a.searchimg(href=url)
				.imgbox(style=img)
			.search_datainfo
				h3
					a(href=url)=name
				block

block breadcrumb
	.col-xs-12
		ol.breadcrumb.text-right
			li
				a(href='index.html') HiClicks
			li.active 搜尋列表

block sidenav
	include include/_blog_sidebar.jade

